<body class="stretched no-transition">
    <div id="fb-root"></div>
    <script>
        (function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) return;
            js = d.createElement(s); js.id = id;
            js.src = "//connect.facebook.net/vi_VN/sdk.js#xfbml=1&version=v2.8&appId=1836521173301566";
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));
    </script>
    <div id="menu-wrapper">
        <div id="sidebar-wrapper">
            <ul class="sidebar-nav">
                <div id="top-search-sm">
                    <form method="get" action="/tim-kiem.html">
                        <input type="hidden" name="type" value="product" />
                        <input type="text" name="keyword" class="form-control" placeholder="Tìm kiếm..">
                    </form>
                </div>
                @{ Html.RenderAction("SearchCategoryMobile", "Home");}
            </ul>
        </div>
        <a href="#menu-toggle" class="btn btn-default" id="menu-toggle"><i class='icon-reorder'></i></a>
    </div>
    <div id="wrapper" class="clearfix">
        @RenderSection("left_menu", required: true)
        @{ Html.RenderAction("TopBar", "Home");}
        <header id="header">
            <div id="header-wrap">
                <div class="container clearfix">
                    <div id="logo" class='col-md-3'>
                        <a href="/" class="standard-logo" data-dark-logo="//hstatic.net/198/1000055198/1000144003/logo.png"><img src="/Assets/client/hstatic.net/198/1000055198/1000144003/logoa6f6.png" alt="The Shoes"></a>
                        <a href="/" class="retina-logo" data-dark-logo="//hstatic.net/198/1000055198/1000144003/logo.png"><img src="/Assets/client/hstatic.net/198/1000055198/1000144003/logoa6f6.png" alt="The Shoes"></a>
                    </div>
                    <div class='top_search col-md-9'>
                        <form class='col-md-9 form-inline hidden-xs hidden-sm' method="get" action="/tim-kiem.html">
                            <div class='form-group col-md-12'>
                                <input type="text" id="txtKeyword" name="keyword" class="form-control col-md-8 search_input" placeholder="Tìm kiếm..">
                                <select class='collection col-md-3 search_collection'>
                                    @{ Html.RenderAction("SearchCategory", "Home");}
                                </select>
                                <a class='button col-md-1' href='#'><i class='icon-search3'></i></a>
                            </div>
                        </form>
                        <div id="top-cart" class='top-cart-block col-md-3'>
                            @{ Html.RenderAction("HeaderCart", "Home");}
                        </div>
                    </div>
                    <a href='#' id='top_link_trigger' class='hidden-lg hidden-md'><i class='icon-ellipsis-vertical'></i></a>
                    <script>
                        $("#sidebar-wrapper ul li a span").click(function (event) {
                            event.preventDefault();
                            var li = $(this).parents('li');
                            li.children('ul').toggle('slow');
                        });
                    </script>
                </div>
            </div>
        </header>
        @RenderSection("title", required: true)
        <section id="content">
            @RenderBody()
        </section>
        @{ Html.RenderAction("Footer", "Home");}
    </div>
    <div id="gotoTop" class="icon-angle-up"></div>




